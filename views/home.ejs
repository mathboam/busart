<% let tech = 0; %>
<% let tax = 0; %>
<% let food = 0; %>
<% let person = 0; %>
<% let conduct = 0; %>
<% let invest = 0; %>
<% let other = 0; %>

<% verifiedArticles.forEach(function(article){ %>

  <% if(article.category === "Tax"){%>
    <% tax = tax + 1 %>

  <% } %>
  <% if(article.category == "conduct"){%>
    <% conduct = conduct + 1 %>

  <% } %>
  <% if(article.category == "personal"){%>
    <% person = person+ 1 %>
  <% } %>
  <% if(article.category == "IT"){%>
    <% tech = tech + 1 %>
  <% } %>
  <% if(article.category == "investment"){%>
    <% invest = invest +1 %>
  <% } %>
  <% if(article.category == "other"){%>
    <% other = other +1 %>
  <% } %>

<% }); %>

